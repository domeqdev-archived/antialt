<%- include('../blocks/header', {bot, user, path}) %>

<div class="dashboard is-0-8">
  <div class="dashboard-sidebar">
      <div class="sidebar-selector">
          <div class="sidebar-selected">
              <div class="sidebar-guild-icon-container">
                <% if(!guild.icon) { %>
                  <img width="30" height="30" class="rounded-circle" src="/public/df.png">
                <% } else { %>
                  <img width="30" height="30" class="rounded-circle" src="<%= `https://cdn.discordapp.com/icons/${guild.id}/${guild.icon}.png` %>">
                <% } %>
              </div> 
              <div class="sidebar-text"><%- guild.name %></div>
          </div> 
      </div> 
      <div class="sidebar-body">
        <div class="sidebar-item-row">
          <a href="/dashboard/<%- guild.id %>" class="sidebar-item active">
              <img src="data:image/svg+xml;base64," class="sidebar-item-icon">
              <span>Overview</span>
          </a> 
        </div>
        <div class="sidebar-item-row"> 
          <a href="/dashboard/<%- guild.id %>/antialt" class="sidebar-item">
              <img src="data:image/svg+xml;base64," class="sidebar-item-icon"> 
              <span>AntiAlt</span>
          </a>
        </div>
        <div class="sidebar-item-row">
          <a href="/dashboard/<%- guild.id %>/captcha" class="sidebar-item">
              <img src="data:image/svg+xml;base64," class="sidebar-item-icon"> 
              <span>Verification</span>
          </a>
        </div>
        <div class="sidebar-item-row">
          <a href="/dashboard/<%- guild.id %>/vanity" class="sidebar-item">
              <img src="data:image/svg+xml;base64," class="sidebar-item-icon"> 
              <span>Vanity Link</span>
          </a> 
        </div>
    </div> 
  </div>
  <div class="dashboard-body">
    <% if(!guild.me.hasPermission("ADMINISTRATOR")) { %>
    <div class="dash-item dashboard-premium">
      <div class="premium-text">
        <span class="title">Missing Admin Permissions!</span><br>
        <span class="subtitle">Bot can't work properly without admin permissions.</span>
      </div>
    </div>
    <% } %>
      <center>
        <h1>ðŸ§· Overview</h1>
        <p style="font-size: 15px;">ðŸ‘‹ Welcome <%- user.username %> to AntiAlt Dashboard!</p>
        <p>ðŸ§  If you need help, join our support server:</p>
        <iframe src="https://invitewidget.ct8.pl/js?invite=8jQP52j" width="410" height="190" allowtransparency="true" frameborder="0"></iframe>
      </center>
      <div class="dash-item">
        <div class="overview-stats">
          <div class="column two">
            <div class="icon">
              <img src="data:image/svg+xml;base64,">
            </div> 
            <div class="text">
              <span class="subtitle"><b>Members:</b> <%- guild.memberCount %><br><b>Owner:</b> <%= user.username %>#<%= user.discriminator %></span>
            </div>
          </div> 
          <div class="column three">
            <div class="icon">
              <img src="data:image/svg+xml;base64,">
            </div> 
            <div class="text">
              <span class="subtitle"><b>Ping:</b> <%- Math.round(bot.ws.ping) %> ms<br><b>Shard:</b> <%- guild.shardID %></span>
            </div>
          </div>
        </div>
      </div>

  </div>
</div>

<%- include('../blocks/footer') %>